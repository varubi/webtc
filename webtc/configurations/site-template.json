 {
   "Basepath": "_directorypath",
   "ClearCache": false,
   "DisplayErrors": false,
   "ErrorPages": {
     "*HTMLStatusCode": "*page.html"
   },
   "DynamicFileExtensions": ["*ext"],
   "URLRewrites": [
     {
       "Filter": "_regex",
       "Match": "_regex",
       "Replace": "*regex"
     }
   ],
   "IndexFileNames": ["*file.ext"],
   "MIMETypes": {
     "*ext": "*mimeType"
   },
   "EventListeners": {
     "OnResponseStart": "_function",
     "OnResponseEnd": "_function"
   }
 }
